    <footer n:if="$thereIsFooter" class="bg-dark p-4 d-flex justify-content-around align-items-center flex-wrap">
        {include '../components/footer.latte'}
    </footer>
   
    <script src="https://kit.fontawesome.com/a274ff346d.js" crossorigin="anonymous"></script>
    <script type="module" src="http://localhost:5173/@vite/client"></script>
    <script type="module" src="http://localhost:5173/app.js"></script>
